#include <bits/stdc++.h> 
using namespace std;

void solve() {
    int n;
    cin>>n;
    int f=0;
    for(int i=0;i<n;i++) {
        int k;
        cin>>k;
        if(k==1)
            f=1;
    }
    if(f==0)
        cout<<"EASY"<<endl;
    else
        cout<<"HARD"<<endl;
}
int main()
{
    int t=1;
    // cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
}
