#include <bits/stdc++.h> 
using namespace std;

void solve() {
    int n, m;
    cin>>n>>m;
    char ch;
    int ans = 0;
    string str[n];
    for(int i = 0; i<n; i++)
    {
        cin>>str[i];
        if(i==n-1)
        {
            for(int j = 0; j<m;j++)
            {
                if(str[i][j] == 'D')
                    ans++;
            }
        }
        else if(str[i][m-1] == 'R')
            ans++;
    }
    cout<<ans<<endl;
}
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
}
