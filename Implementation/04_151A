#include <bits/stdc++.h> 
using namespace std;

void solve() {
    int n,k,l,c,d,p,nl,np;
    cin>>n>>k>>l>>c>>d>>p>>nl>>np;
    int a1 = (k*l)/nl;
    int a2 = c*d;
    int a3 = p/np;
    int ans = min(a3, min(a1,a2));
    cout<<ans/n<<endl;
}
int main()
{
    int t=1;
    // cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
}
