#include <bits/stdc++.h> 
using namespace std;
bool isCons(char c)
{
    bool isLowercaseVowel = (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u' || c=='y');

    // evaluates to 1 (true) if c is an uppercase vowel
    bool isUppercaseVowel = (c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U' || c=='Y');

    return (isUppercaseVowel || isLowercaseVowel);
}
void solve() {
    string str;
    cin>>str;
    string ans="";
    for(int i=0;i<str.size();i++)
    {
        if(!isCons(str[i])) {
            ans = ans+".";
            ans.push_back(tolower(str[i]));
        }
    }
    cout<<ans<<endl;
}
int main()
{
    int t=1;
    // cin>>t;
    while(t--)
    {
        solve();
    }
    return 0;
}
